    <parent link="upper_leg_rl"/>
    <child link="lower_leg_rl"/>
    <origin xyz="0 0 -0.1"/> <!-- Bottom of upper_leg_rl -->
    <axis xyz="0 1 0"/> <!-- Adjust to control ankle rotation direction -->
    <limit effort="1" lower="-1.57" upper="1.57" velocity="1"/>
  </joint>


    <!-- === REAR RIGHT LEG === -->
  <link name="hip_rr">
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.0001" iyy="0.0001" izz="0.0001" ixy="0.0" ixz="0.0" iyz="0.0"/>
      <origin xyz="-0.15 -0.1 -0.025"/>
    </inertial>
