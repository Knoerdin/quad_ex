      <inertia ixx="0.0001" iyy="0.0001" izz="0.0001" ixy="0.0" ixz="0.0" iyz="0.0"/>
      <origin xyz="0 0 -0.05"/>
    </inertial>
    <visual>
      <geometry><cylinder radius="0.02" length="0.1"/></geometry>
      <origin xyz="0 0 -0.05"/>
    </visual>
    <collision>
      <geometry><cylinder radius="0.02" length="0.1"/></geometry>
      <origin xyz="0 0 -0.05"/>
