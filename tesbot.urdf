  </link>

  <joint name="hip_joint_rr" type="fixed">
    <parent link="body"/>
    <child link="hip_rr"/>
    <origin xyz="-0.15 -0.1 -0.025"/>
  </joint>

  <!-- Upper leg segment -->
  <link name="upper_leg_rr">
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.0001" iyy="0.0001" izz="0.0001" ixy="0.0" ixz="0.0" iyz="0.0"/>
      <origin xyz="0 0 -0.05"/>
    </inertial>
    <visual>
      <geometry><cylinder radius="0.02" length="0.1"/></geometry>
