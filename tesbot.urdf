<?xml version="1.0" ?>
<robot name="simple_quadruped">

  <!-- Main Body -->
  <link name="body">
    <inertial>
      <mass value="5.0"/>
      <inertia ixx="0.1" iyy="0.1" izz="0.1" ixy="0.0" ixz="0.0" iyz="0.0"/>
      <origin xyz="0 0 0" />
    </inertial>
    <visual>
      <geometry><box size="0.4 0.2 0.1"/></geometry>
      <origin xyz="0 0 0" />
    </visual>
    <collision>
      <geometry><box size="0.4 0.2 0.1"/></geometry>
      <origin xyz="0 0 0"/>
    </collision>
  </link>

  <!-- === FRONT LEFT LEG SPLIT AT ANKLE === -->
  <link name="hip_fl">
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.0001" iyy="0.0001" izz="0.0001" ixy="0.0" ixz="0.0" iyz="0.0"/>
      <origin xyz="0 0 0"/>
    </inertial>
    <visual>
      <geometry><box size="0.05 0.05 0.05"/></geometry>
    </visual>
    <collision>
      <geometry><box size="0.05 0.05 0.05"/></geometry>
    </collision>
  </link>

  <joint name="hip_joint_fl" type="fixed">
    <parent link="body"/>
    <child link="hip_fl"/>
    <origin xyz="0.15 0.1 -0.025"/>
  </joint>

  <!-- Upper leg segment -->
  <link name="upper_leg_fl">
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.0001" iyy="0.0001" izz="0.0001" ixy="0.0" ixz="0.0" iyz="0.0"/>
      <origin xyz="0 0 -0.05"/>
    </inertial>
    <visual>
      <geometry><cylinder radius="0.02" length="0.1"/></geometry>
      <origin xyz="0 0 -0.05"/>
    </visual>
    <collision>
      <geometry><cylinder radius="0.02" length="0.1"/></geometry>
      <origin xyz="0 0 -0.05"/>
    </collision>
  </link>

  <joint name="knee_joint_fl" type="revolute">
    <parent link="hip_fl"/>
    <child link="upper_leg_fl"/>
    <origin xyz="0 0 -0.025"/>
    <axis xyz="0 1 0"/>
    <limit effort="1" lower="-1.57" upper="1.57" velocity="1"/>
  </joint>

  <!-- Lower leg segment -->
  <link name="lower_leg_fl">
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.0001" iyy="0.0001" izz="0.0001" ixy="0.0" ixz="0.0" iyz="0.0"/>
      <origin xyz="0 0 -0.05"/>
    </inertial>
    <visual>
      <geometry><cylinder radius="0.02" length="0.1"/></geometry>
      <origin xyz="0 0 -0.05"/>
    </visual>
    <collision>
      <geometry><cylinder radius="0.02" length="0.1"/></geometry>
      <origin xyz="0 0 -0.05"/>
    </collision>
  </link>

  <!-- Ankle joint between upper and lower leg -->
  <joint name="ankle_joint_fl" type="revolute">
    <parent link="upper_leg_fl"/>
    <child link="lower_leg_fl"/>
    <origin xyz="0 0 -0.1"/>
    <axis xyz="0 1 0"/>
    <limit effort="1" lower="-1.57" upper="1.57" velocity="1"/>
  </joint>

  <!-- === FRONT RIGHT LEG SPLIT AT ANKLE === -->
  <link name="hip_fr">
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.0001" iyy="0.0001" izz="0.0001" ixy="0.0" ixz="0.0" iyz="0.0"/>
      <origin xyz="0 0 0"/>
    </inertial>
    <visual>
      <geometry><box size="0.05 0.05 0.05"/></geometry>
    </visual>
    <collision>
      <geometry><box size="0.05 0.05 0.05"/></geometry>
    </collision>
  </link>

  <joint name="hip_joint_fr" type="fixed">
    <parent link="body"/>
    <child link="hip_fr"/>
    <origin xyz="0.15 -0.1 -0.025"/>
  </joint>

  <!-- Upper leg segment -->
  <link name="upper_leg_fr">
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.0001" iyy="0.0001" izz="0.0001" ixy="0.0" ixz="0.0" iyz="0.0"/>
      <origin xyz="0 0 -0.05"/>
    </inertial>
    <visual>
      <geometry><cylinder radius="0.02" length="0.1"/></geometry>
      <origin xyz="0 0 -0.05"/>
    </visual>
    <collision>
      <geometry><cylinder radius="0.02" length="0.1"/></geometry>
      <origin xyz="0 0 -0.05"/>
    </collision>
  </link>

  <joint name="knee_joint_fr" type="revolute">
    <parent link="hip_fr"/>
    <child link="upper_leg_fr"/>
    <origin xyz="0 0 -0.025"/>
    <axis xyz="0 1 0"/>
    <limit effort="1" lower="-1.57" upper="1.57" velocity="1"/>
  </joint>

  <!-- Lower leg segment -->
  <link name="lower_leg_fr">
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.0001" iyy="0.0001" izz="0.0001" ixy="0.0" ixz="0.0" iyz="0.0"/>
      <origin xyz="0 0 -0.05"/>
    </inertial>
    <visual>
      <geometry><cylinder radius="0.02" length="0.1"/></geometry>
      <origin xyz="0 0 -0.05"/>
    </visual>
    <collision>
      <geometry><cylinder radius="0.02" length="0.1"/></geometry>
      <origin xyz="0 0 -0.05"/>
    </collision>
  </link>

  <!-- Ankle joint between upper and lower leg -->
  <joint name="ankle_joint_fr" type="revolute">
    <parent link="upper_leg_fr"/>
    <child link="lower_leg_fr"/>
    <origin xyz="0 0 -0.1"/>
    <axis xyz="0 1 0"/>
    <limit effort="1" lower="-1.57" upper="1.57" velocity="1"/>
  </joint>

   <!-- === REAR LEFT LEG === -->
  <link name="hip_rl">
    <inertial>
      <mass value="0.1"/>
      <inertia ixx="0.0001" iyy="0.0001" izz="0.0001" ixy="0.0" ixz="0.0" iyz="0.0"/>
      <origin xyz="-0.15 0.1 -0.025"/>
    </inertial>
    <visual>
      <geometry><box size="0.05 0.05 0.05"/></geometry>
    </visual>
    <collision>
      <geometry><box size="0.05 0.05 0.05"/></geometry>
    </collision>
  </link>

  <joint name="hip_joint_rl" type="fixed">
    <parent link="body"/>
    <child link="hip_rl"/>
    <origin xyz="-0.15 0.1 -0.025"/>
  </joint>

  <!-- Upper leg segment -->
  <link name="upper_leg_rl">
    <inertial>
