  <joint name="knee_joint_rr" type="revolute">
    <parent link="hip_rr"/>
    <child link="upper_leg_rr"/>
    <origin xyz="0 0 -0.025"/>
    <axis xyz="0 1 0"/>
    <limit effort="1" lower="-1.57" upper="1.57" velocity="1"/>
  </joint>

  <!-- Lower leg segment -->
  <link name="lower_leg_rr">
    <inertial>
